{
  "name": "layhold-infra",
  "version": "1.0.0",
  "main": "main.js",
  "types": "main.ts",
  "license": "MPL-2.0",
  "private": true,
  "scripts": {
    "get": "cdktf get",
    "build": "tsc",
    "synth": "cdktf synth",
    "compile": "tsc --pretty",
    "watch": "tsc -w",
    "test": "npm run lint && jest",
    "test:watch": "jest --watch",
    "upgrade": "npm i cdktf@latest cdktf-cli@latest",
    "upgrade:next": "npm i cdktf@next cdktf-cli@next",
    "lint": "eslint ./main.ts jest.config.js setup.js './lib/*.ts' './lib/**/*.ts' '__tests__/*.ts' '__tests__/**/*.ts'",
    "lint-fix": "npm run lint -- --fix",
    "login": "gcloud auth application-default login && gcloud auth login && gcloud auth configure-docker \"${GCP_REGION:-us-central1}-docker.pkg.dev\""
  },
  "engines": {
    "node": ">=14.0"
  },
  "dependencies": {
    "@cdktf/provider-google": "6.0.0",
    "cdktf": "^0.15.5",
    "constructs": "^10.3.0"
  },
  "devDependencies": {
    "@types/jest": "^29.5.12",
    "@types/node": "^20.11.19",
    "@typescript-eslint/eslint-plugin": "^7.0.1",
    "@typescript-eslint/parser": "^7.0.1",
    "eslint": "^8.56.0",
    "eslint-config-google": "^0.14.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  }
}
